{
  "name": "admin-server",
  "version": "1.0.0",
  "main": "index.js",
  "description": "",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc -p .",
    "make-model": "npx sequelize-cli model:generate --name company_employees --attributes email:string",
    "add-column": "npx sequelize-cli migration:create --name modify_add_new_fields",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "migrate": "npx sequelize-cli db:migrate",
    "downSingle": "sequelize db:migrate:undo --name 20221016154644-create-employee.js",
    "down": "npx sequelize-cli db:migrate:undo:all"
  },
